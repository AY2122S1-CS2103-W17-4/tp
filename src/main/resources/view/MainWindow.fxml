<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.Scene?>
<?import javafx.scene.text.Font?>
<?import javafx.stage.Stage?>
<fx:root minHeight="675" minWidth="1200" onCloseRequest="#handleExit" title="Uni-Fy" type="javafx.stage.Stage" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
  <icons>
    <Image url="@/images/unify.png" />
  </icons>
  <scene>
    <Scene>
      <stylesheets>
        <URL value="@Fonts.css" />
        <URL value="@DarkTheme.css" />
        <URL value="@Extensions.css" />
      </stylesheets>

      <VBox>
        <MenuBar fx:id="menuBar" VBox.vgrow="NEVER">
          <Menu mnemonicParsing="false" text="File">
            <MenuItem mnemonicParsing="false" onAction="#handleExit" text="Exit" />
          </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <MenuItem fx:id="helpMenuItem" mnemonicParsing="false" onAction="#handleHelp" text="Help" />
          </Menu>
        </MenuBar>
            <HBox minWidth="500.0" prefWidth="500.0" styleClass="pane-with-border" VBox.vgrow="ALWAYS">
               <children>

              <VBox fx:id="taskList" minWidth="340" prefWidth="340" styleClass="pane-with-border">
                <padding>
                  <Insets bottom="10" left="10" right="10" top="10" />
                </padding>
                     <Label alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0" minWidth="320.0" prefWidth="320.0" text="Task List" textAlignment="CENTER" textFill="WHITE">
                        <font>
                           <Font size="14.0" />
                        </font>
                        <padding>
                           <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                        </padding></Label>
                <StackPane fx:id="taskListPanelPlaceholder" VBox.vgrow="ALWAYS" />
              </VBox>
                  <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
                     <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0" minWidth="420.0" prefWidth="1540.0" text="Week X" textAlignment="CENTER" textFill="WHITE" VBox.vgrow="ALWAYS">
                           <font>
                              <Font size="14.0" />
                           </font>
                        </Label>

                        <HBox prefHeight="180.0" prefWidth="490.0" VBox.vgrow="ALWAYS">
                           <children>
                              <VBox fx:id="dailyPanel1" minWidth="70.0" prefWidth="220.0" styleClass="pane-with-border">
                                 <padding>
                                    <Insets bottom="10" left="10" right="10" top="10" />
                                 </padding>
                                 <children>
                                    <Label alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0" minWidth="50.0" prefWidth="200.0" text="Monday" textAlignment="CENTER" textFill="WHITE">
                                       <font>
                                          <Font size="14.0" />
                                       </font>
                                       <padding>
                                          <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                       </padding>
                                    </Label>
                                    <StackPane fx:id="dailyPanelPlaceholder1" VBox.vgrow="ALWAYS" />
                                 </children>
                              </VBox>
                              <VBox fx:id="dailyPanel2" minWidth="70.0" prefWidth="220.0" styleClass="pane-with-border">
                                 <padding>
                                    <Insets bottom="10" left="10" right="10" top="10" />
                                 </padding>
                                 <children>
                                    <Label alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0" minWidth="50.0" prefWidth="200.0" text="Tuesday" textAlignment="CENTER" textFill="WHITE">
